    <ImageView
        android:id="@+id/eg_location_black"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="200dp"
        android:layout_marginTop="208dp"
        android:contentDescription="@string/eg_location_black"
        app:layout_constraintLeft_toLeftOf="@+id/campus_Image"
        app:layout_constraintTop_toTopOf="@+id/campus_Image"
        app:srcCompat="@drawable/loacation_black" />

    <ImageView
        android:id="@+id/st_location_black"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="280dp"
        android:layout_marginTop="168dp"
        android:contentDescription="@string/st_location_black"
        app:layout_constraintLeft_toLeftOf="@+id/campus_Image"
        app:layout_constraintTop_toTopOf="@+id/campus_Image"
        app:srcCompat="@drawable/loacation_black" />

    <ImageView
        android:id="@+id/lb_location_black"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="272dp"
        android:layout_marginTop="12dp"
