        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="@+id/campus_Image"
        app:layout_constraintLeft_toLeftOf="@+id/campus_Image"
        android:layout_marginLeft="272dp"
        android:layout_marginTop="12dp"
        android:contentDescription="@string/lb_location_red"
        android:visibility="invisible"
        app:srcCompat="@drawable/location_red"
        tools:layout_editor_absoluteX="48dp" />
    <ImageView
        android:id="@+id/lake_location_red"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="@+id/campus_Image"
        app:layout_constraintLeft_toLeftOf="@+id/campus_Image"
        android:layout_marginLeft="236dp"
        android:layout_marginTop="108dp"
        android:contentDescription="@string/lake_location_red"
        android:visibility="invisible"
        app:srcCompat="@drawable/location_red"
