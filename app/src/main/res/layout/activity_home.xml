        android:layout_marginTop="232dp"
        android:contentDescription="@string/st_location_black"
        app:layout_constraintLeft_toLeftOf="@+id/campus_Image"
        app:layout_constraintTop_toTopOf="@+id/campus_Image"
        app:srcCompat="@drawable/loacation_black" />

    <ImageView
        android:id="@+id/lb_location_black"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="464dp"
        android:layout_marginTop="16dp"
        android:contentDescription="@string/lb_location_black"
        app:layout_constraintLeft_toLeftOf="@+id/campus_Image"
        app:layout_constraintTop_toTopOf="@+id/campus_Image"
        app:srcCompat="@drawable/loacation_black" />

    <ImageView
        android:id="@+id/lake_location_black"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="428dp"
        android:layout_marginTop="92dp"
        android:contentDescription="@string/lake_location_black"
        app:layout_constraintLeft_toLeftOf="@+id/campus_Image"
        app:layout_constraintTop_toTopOf="@+id/campus_Image"
        app:srcCompat="@drawable/loacation_black" />

    <ImageView
        android:id="@+id/ct_location_red"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="232dp"
        android:layout_marginTop="264dp"
        android:contentDescription="@string/ct_location_red"
        android:visibility="invisible"
        app:layout_constraintLeft_toLeftOf="@+id/campus_Image"
        app:layout_constraintTop_toTopOf="@+id/campus_Image"
        app:srcCompat="@drawable/location_red" />

    <ImageView
        android:id="@+id/eg_location_red"
        android:layout_width="61dp"
        android:layout_height="60dp"
        android:layout_marginLeft="328dp"
        android:layout_marginTop="204dp"
        android:contentDescription="@string/eg_location_red"
        android:visibility="invisible"
        app:layout_constraintLeft_toLeftOf="@+id/campus_Image"
        app:layout_constraintTop_toTopOf="@+id/campus_Image"
        app:srcCompat="@drawable/location_red" />

    <ImageView
        android:id="@+id/foodHall_location_red"
        android:layout_width="61dp"
        android:layout_height="60dp"
        android:layout_marginLeft="376dp"
        android:layout_marginTop="164dp"
        android:contentDescription="@string/eg_location_red"
        android:visibility="invisible"
        app:layout_constraintLeft_toLeftOf="@+id/campus_Image"
        app:layout_constraintTop_toTopOf="@+id/campus_Image"
        app:srcCompat="@drawable/location_red" />

    <ImageView
        android:id="@+id/st_location_red"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="416dp"
        android:layout_marginTop="232dp"
        android:contentDescription="@string/st_location_red"
        android:visibility="invisible"
        app:layout_constraintLeft_toLeftOf="@+id/campus_Image"
        app:layout_constraintTop_toTopOf="@+id/campus_Image"
        app:srcCompat="@drawable/location_red" />

    <ImageView
        android:id="@+id/lb_location_red"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="464dp"
        android:layout_marginTop="16dp"
        android:contentDescription="@string/lb_location_red"
        android:visibility="invisible"
        app:layout_constraintLeft_toLeftOf="@+id/campus_Image"
        app:layout_constraintTop_toTopOf="@+id/campus_Image"
