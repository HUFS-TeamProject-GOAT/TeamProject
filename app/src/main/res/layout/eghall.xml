<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <ImageView
        android:id="@+id/eg_Background"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:contentDescription="background"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />



    <ImageButton
        android:id="@+id/go_egHall2btn"
        android:layout_width="87dp"
        android:layout_height="69dp"
        android:layout_marginEnd="192dp"
        android:layout_marginBottom="340dp"
        android:backgroundTint="@android:color/transparent"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/thing" />

    <ImageButton
        android:id="@+id/go_egHall3btn"
        android:layout_width="85dp"
        android:layout_height="65dp"
        android:layout_marginEnd="128dp"
        android:layout_marginBottom="360dp"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:backgroundTint="@android:color/transparent"
        android:scaleType="fitXY"
        app:srcCompat="@drawable/top" />

    <ImageButton
        android:id="@+id/go_egHall4btn"
        android:layout_width="95dp"
        android:layout_height="76dp"
        android:layout_marginEnd="56dp"
        android:layout_marginBottom="376dp"
        android:backgroundTint="@android:color/transparent"
        android:scaleType="fitXY"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/thing" />

    <ImageButton
        android:id="@+id/go_egHall5btn"
        android:layout_width="94dp"
        android:layout_height="72dp"
        android:layout_marginEnd="276dp"
        android:layout_marginBottom="388dp"
        android:backgroundTint="@android:color/transparent"
        android:scaleType="fitXY"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/thing" />

    <ImageButton
        android:id="@+id/egHall_last_btn"
        android:layout_width="118dp"
        android:layout_height="81dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="224dp"
        android:backgroundTint="@android:color/transparent"
        android:scaleType="fitXY"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/thing" />

    <ImageButton
        android:id="@+id/go_home_btn"
        style="@style/Widget.AppCompat.ImageButton"
        android:layout_width="97dp"
        android:layout_height="53dp"
        android:layout_marginStart="176dp"
        android:layout_marginTop="648dp"
        android:backgroundTint="@android:color/transparent"
        android:scaleType="fitXY"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/bottom" />

</androidx.constraintlayout.widget.ConstraintLayout>