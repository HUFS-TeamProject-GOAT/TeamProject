<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <ImageView
        android:id="@+id/maincharacter"
        android:layout_width="228dp"
        android:layout_height="210dp"
        app:layout_constraintBottom_toTopOf="@id/userName"
        app:layout_constraintHorizontal_bias="0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintVertical_bias="0.302"
        app:srcCompat="@drawable/maincharacter" />

    <ImageView
        android:id="@+id/hyerim"
        android:layout_width="220dp"
        android:layout_height="210dp"
        app:layout_constraintBottom_toTopOf="@id/name_hyerim"
        app:layout_constraintEnd_toEndOf="parent"
        android:visibility="invisible"
        app:srcCompat="@drawable/hyerim" />
    <ImageView
        android:id="@+id/minsu"
        android:layout_width="208dp"
        android:layout_height="210dp"
        app:layout_constraintBottom_toTopOf="@id/name_minsu"
        app:layout_constraintEnd_toEndOf="parent"
        android:visibility="invisible"
        app:srcCompat="@drawable/minsu" />

    <EditText
        android:id="@+id/userName"
        android:layout_width="122dp"
        android:layout_height="59dp"
        android:layout_marginStart="4dp"
        android:autofillHints="name"
        android:inputType="text"
        android:background="@null"
        android:visibility="invisible"
        android:textAppearance="@style/TextAppearance.AppCompat.Display1"
        app:layout_constraintBottom_toTopOf="@+id/st_storyText2_1"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/name_hyerim"
        android:layout_width="122dp"
        android:layout_height="59dp"
        android:layout_marginEnd="12dp"
        android:autofillHints="name"
        android:text="록시"
        android:gravity="right"
        android:textAppearance="@style/TextAppearance.AppCompat.Display1"
        android:visibility="invisible"
        app:layout_constraintBottom_toTopOf="@+id/st_storyText2_1"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/name_minsu"
        android:layout_width="122dp"
        android:layout_height="59dp"
        android:layout_marginEnd="12dp"
        android:autofillHints="name"
        android:text="케인"
        android:gravity="right"
        android:textAppearance="@style/TextAppearance.AppCompat.Display1"
        android:visibility="invisible"
        app:layout_constraintBottom_toTopOf="@+id/st_storyText2_1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/maincharacter" />

